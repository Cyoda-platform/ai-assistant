{
  "initial_state": "none",
  "workflow_name": "add_new_entity_for_existing_app",
  "states": {
    "none": {
      "transitions": {
        "start_data_entity_add": {
          "next": "generate_data_model"
        }
      }
    },
    "generate_data_model": {
      "transitions": {
        "initial_notification": {
          "next": "user_notification",
          "action": {
            "name": "process_event",
            "config": {
              "type": "prompt",
              "model": {
                "model_name": "gpt-4o-mini",
                "temperature": 0.7,
                "max_tokens": 10000
              },
              "messages": [
                {
                  "role": "user",
                  "content": [
                    "Please add data model example for the {entity_name} according to the user requirement: {user_request}.",
                    "Please return raw data example and not schema."
                  ]
                }
              ],
              "response_format": {
                "name": "entities_design_schema",
                "description": "entities design schema",
                "schema": {
                  "type": "object",
                  "properties": {
                  },
                  "required": [
                  ]
                }
              },
              "publish": true,
              "output": {
                "local_fs": [
                  "entity/{entity_name}/{entity_name}.json"
                ]
              }
            }
          }
        }
      }
    },
    "user_notification": {
      "transitions": {
        "finish": {
          "next": "locked_chat",
          "action": {
            "name": "notify_user",
            "config": {
              "type": "question",
              "question": "Please let me know if I can help!",
              "publish": true
            }
          }
        }
      }
    }
  }
}
